{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid",
        "processing",
        "completed",
        "refunded",
        "canceled",
        "hold"
      ]
    },
    "total": {
      "type": "decimal"
    },
    "checkout_session": {
      "type": "string"
    },
    "product": {
      "via": "orders",
      "model": "product"
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    }
  }
}
